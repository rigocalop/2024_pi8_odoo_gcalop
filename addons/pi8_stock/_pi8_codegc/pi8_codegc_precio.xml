<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="pi8_codegc_precio_form" model="ir.ui.view">
        <field name="name">Formulario (PI8-CodeGC) de Precios</field>
        <field name="model">pi8.codegc.precio</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field string="Clave.:" name="key"  required="1" default_focus="1"/>
                        <field string="Nombre:" name="name" required="1"/>
                        <field string="Valor:" name="value" required="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="pi8_codegc_precio_tree" model="ir.ui.view">
        <field name="name">Listado (PI8-CodeGC) de Precios</field>
        <field name="model">pi8.codegc.precio</field>
        <field name="arch" type="xml">
            <tree>
                <field name="key"/>
                <field name="name"/>
                <field name="value"/>
            </tree>
        </field>
    </record>
</odoo>